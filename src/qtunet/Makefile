dist: 
	@mkdir -p tmp/obj/qtunet && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) tmp/obj/qtunet/ && $(COPY_FILE) --parents dlg_main.ui.h dlg_about.ui.h tmp/obj/qtunet/ && ( cd `dirname tmp/obj/qtunet` && $(TAR) qtunet.tar qtunet && $(GZIP) qtunet.tar ) && $(MOVE) `dirname tmp/obj/qtunet`/qtunet.tar.gz . && $(DEL_FILE) -r tmp/obj/qtunet

