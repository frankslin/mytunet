####### Install

install_qtunet: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	-$(INSTALL_PROGRAM) /usr/home/henryhu/soft/mytunet/src/qtunet-qt4/qtunet $(INSTALL_ROOT)/usr/local/bin/


uninstall_qtunet:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/bin/qtunet
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/bin/ 


install_translation: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/qtunet/lang/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/qtunet/lang/ 
	-$(INSTALL_FILE) /usr/home/henryhu/soft/mytunet/src/qtunet-qt4/qtunet_zh_CN.qm $(INSTALL_ROOT)/usr/local/share/qtunet/lang/


uninstall_translation:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/qtunet/lang/qtunet_zh_CN.qm
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/qtunet/lang/ 


install:  install_qtunet install_translation  FORCE

uninstall: uninstall_qtunet uninstall_translation   FORCE

FORCE:

